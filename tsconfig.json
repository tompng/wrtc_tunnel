{
  "compilerOptions": {
    "strict": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "node"
    ],
  }
}
